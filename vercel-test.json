{
  "version": 2,
  "builds": [
    {
      "src": "api/test.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/test.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  }
} 